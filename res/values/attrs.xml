<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="Common">
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="progress" format="integer" />
        <attr name="min" format="integer" />
        <attr name="max" format="integer" />
        <attr name="wheelSize" format="dimension" />
        <attr name="wheelColor" format="color" />
        <attr name="wheelZeroAngle" format="integer" />
        <attr name="wheelWholeAngle" format="integer" />
        <attr name="wheelDirection" format="enum">
            <enum name="clockwise" value="0" />
            <enum name="counterclockwise" value="1" />
        </attr>
        <attr name="pointerColor" format="color" />
    </declare-styleable>
    <declare-styleable name="BiaxialSeekBar">
        <attr name="x_progress" format="integer" />
        <attr name="x_min" format="integer" />
        <attr name="x_max" format="integer" />
        <attr name="y_progress" format="integer" />
        <attr name="y_min" format="integer" />
        <attr name="y_max" format="integer" />
        <attr name="pointerSize" format="dimension" />
        <attr name="pointerColor" />
        <attr name="android:background" />
    </declare-styleable>
    <declare-styleable name="ContinuousOvalSeekBar">
        <attr name="wheelSize" />
        <attr name="wheelColor" />
        <attr name="wheelZeroAngle" />
        <attr name="wheelWholeAngle" />
        <attr name="wheelDirection" />
        <attr name="android:textSize" />
        <attr name="android:textColor" />
        <attr name="android:progress" />
        <attr name="min" />
        <attr name="android:max" />
    </declare-styleable>
    <declare-styleable name="OvalSeekBar">
        <attr name="wheelSize" />
        <attr name="wheelColor" />
        <attr name="wheelZeroAngle" />
        <attr name="wheelWholeAngle" />
        <attr name="wheelDirection" />
        <attr name="pointerColor" />
        <attr name="android:textSize" />
        <attr name="android:textColor" />
        <attr name="android:progress" />
        <attr name="min" />
        <attr name="android:max" />
        <attr name="pointerSizeAngle" format="integer" />
        <attr name="pointerType" format="enum">
            <enum name="pointer" value="0" />
            <enum name="value" value="1" />
        </attr>
        <attr name="wheelValueSeparatorSizeAngle" format="integer" />
        <attr name="clickableWheel" format="boolean" />
        <attr name="pointerValueLimit" format="boolean" />
        <attr name="android:progressDrawable" />
        <attr name="progressDrawableSize" format="integer" />
    </declare-styleable>

</resources>